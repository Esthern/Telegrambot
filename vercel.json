{
  "version": 2,
  "builds": [
    {
      "src": "Telegrambot/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "Telegrambot/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Telegrambot/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Telegrambot/public/$1"
    }
  ]
}
